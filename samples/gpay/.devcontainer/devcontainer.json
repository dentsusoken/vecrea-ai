{
	"name": "Google Pay Sample Container",
	"dockerComposeFile": [
		"docker-compose.yml"
	],
	"service": "gpay-sample",
	"workspaceFolder": "/workspace/gpay",
	"runServices": [
		"gpay-sample"
	],
	"postCreateCommand": "git config --global --add safe.directory /workspace && /workspace/gpay/.devcontainer/setup.sh",
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-azuretools.vscode-docker",
				"GitHub.vscode-pull-request-github"
			]
		}
	}
}
